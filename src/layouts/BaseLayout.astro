---
import Layout from "./Layout.astro";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";

interface Props {
    title?: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
    <div class="min-h-screen flex flex-col relative">
        <Navbar />
        <slot />
        <Footer />
    </div>
</Layout>
